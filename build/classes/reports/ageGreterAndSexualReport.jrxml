	<parameter name="SexualOrient" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT BENEFICIARIES.STUDENT_REF_NO,
       BENEFICIARIES.GIVEN_NAME,
       BENEFICIARIES.SURNAME,
       BENEFICIARIES.SEX,
       SEXUAL_ORIENTATION.ORIENTATION,
       trunc(months_between(sysdate,BENEFICIARIES.DATE_OF_BIRTH)/12) AS AGE
FROM BENEFICIARIES INNER JOIN ADDITIONAL_INFO ON(BENEFICIARIES.STUDENT_REF_NO=ADDITIONAL_INFO.STUDENT_REF_NO) INNER JOIN SEXUAL_ORIENTATION
ON (SEXUAL_ORIENTATION.SEX_ORIENT_ID=BENEFICIARIES.SEX_ORIENT_ID)
WHERE TO_CHAR(ADDITIONAL_INFO.DATE_REGISTER,'yy/MM/DD') BETWEEN TO_CHAR($P{enrolmentDate1},'yy/MM/DD') AND TO_CHAR($P{enrolmentDate2},'yy/MM/DD') AND ADDITIONAL_INFO.BENEF_TYPE IN('BLF_NONAcc','BLF_NONAcc,BLF_VOL','BLF_VOL') AND trunc(months_between(sysdate,BENEFICIARIES.DATE_OF_BIRTH)/12)>=$P{age1} AND SEXUAL_ORIENTATION.ORIENTATION LIKE  '%' || $P{SexualOrient} || '%'
ORDER BY BENEFICIARIES.SEX]]>
	</queryString>
